<?xml version="1.0" encoding="UTF-8"?>
<target name="gemini-target" includeAllPlatforms="false">
  <!-- Use an Eclipse p2 repository URL (example: the 2021-06 release). You may change the repo to match your target Eclipse version. -->
  <locations>
    <location includeAllPlatforms="false" includeMode="planner" includeConfigurePhase="false" type="InstallableUnit">
      <repository location="https://download.eclipse.org/releases/2021-06"/>
      <units>
        <unit id="org.eclipse.platform"/>
        <unit id="org.eclipse.draw2d"/>
        <unit id="org.eclipse.gef"/>
        <unit id="org.eclipse.zest"/>
      </units>
    </location>
  </locations>
</target>
